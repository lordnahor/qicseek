<div id="news">
   <h2>News</h2>
   <div class="story">
       <input type="button" id="up" class="button"></button>
       <input type="button" id="down" class="button"></button>
       <script type="text/javascript" src="/js/NewsUpDown.class.js"></script>
       <script type="text/javascript">
            new NewsUpDown("up", "down");
